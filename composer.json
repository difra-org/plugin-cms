{
    "name": "difra/plugin-cms",
    "description": "Simple CMS plugin for Difra",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ed Kutuev",
            "email": "ed@napych.com"
        }
    ],
    "dist": {
        "url": "https://github.com/difra-org/plugin-cms/archive/refs/tags/8.0.0.zip",
        "type": "zip"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:difra-org/plugin-cms.git"
        }
    ],
    "require": {
        "difra/difra": "~8.0.0",
        "difra/binding-ckeditor": "~8.0.0"
    },
    "autoload": {
        "files": [
            "main.php"
        ],
        "classmap": [
            "src/CMS.php"
        ],
        "psr-4": {
            "Difra\\CMS\\": "src"
        }
    }
}
